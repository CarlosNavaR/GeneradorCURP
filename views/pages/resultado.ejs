<div class="container shadow align-items-center form-color mt-5 mb-5">
    <p class="text-center mx-auto display-4"><strong>Datos del solicitante</strong></p>
    <hr>
    <div class="row d-flex justify-content-center mt-2">
        <table class="table col-md-6">
            <tbody>
                <tr class="table-borderless">
                    <td><strong>CURP:</strong></td>
                    <td><%-CURP%></td>
                </tr>
                <tr>
                    <td><strong>Nombre(s):</strong></td>
                    <td><%-datos.nombres%></td>
                </tr>
                <tr>
                    <td><strong>Primer apellido:</strong></td>
                    <td><%-datos.ApellidoP%></td>
                </tr>
                <tr>
                    <td><strong>Segundo apellido:</strong></td>
                    <td><%-datos.ApellidoM%></td>
                </tr>
                <tr>
                    <td><strong>Sexo:</strong></td>
                    <td><%-datos.sexo == 'M' ? 'MUJER' : 'HOMBRE'%></td>
                </tr>
                <tr>
                    <td><strong>Fecha de nacimiento:</strong></td>
                    <td><%-datos.Fecha%></td>
                </tr>
                <tr>
                    <td><strong>Entidad de nacimiento:</strong></td>
                    <td><%-estado%></td>
                </tr>
            </tbody>
        </table>
    </div>
    <p class="text-center mt-4 "><a href="/" class="btn btn-outline-info btn-lg mt-2 mb-5">Regresar</a></p>
</div>